{
  "version": 2,
  "framework": null,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "dist" } }
  ],
  "env": {
    "VITE_BACKEND_URL": "${VITE_BACKEND_URL}",
    "VITE_SUPABASE_URL": "${VITE_SUPABASE_URL}",
    "VITE_SUPABASE_ANON_KEY": "${VITE_SUPABASE_ANON_KEY}"
  },
  "routes": [
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
